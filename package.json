{
  "name": "practical-guide-to-fp-ts",
  "description": "Practical Guide to Fp-ts, based on https://dev.to/ryanleecode/series/7325",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/COANelmejor/Practical-Guide-to-Fp-ts.git"
  },
  "scripts": {
    "build": "tsc build",
    "start": "ts-node-dev --clear --respawn src/index.ts",
    "start:p1:pipe": "ts-node-dev --clear --respawn P1_Pipe_and_Flow/pipe.ts",
    "start:p1:flow": "ts-node-dev --clear --respawn P1_Pipe_and_Flow/flow.ts",
    "start:p2:option.map": "ts-node-dev --clear --respawn P2_Option_Map_Flatten_Chain/map.ts",
    "start:p2:option.flatten": "ts-node-dev --clear --respawn P2_Option_Map_Flatten_Chain/flatten.ts",
    "start:p2:option.chain": "ts-node-dev --clear --respawn P2_Option_Map_Flatten_Chain/chain.ts",
    "start:p3:task": "ts-node-dev --clear --respawn P3_Task_Either_TaskEither/task.ts",
    "start:p3:either": "ts-node-dev --clear --respawn P3_Task_Either_TaskEither/either.ts",
    "start:p3:chaining_eithers": "ts-node-dev --clear --respawn P3_Task_Either_TaskEither/chaining_eithers.ts",
    "start:p3:taskeither": "ts-node-dev --clear --respawn P3_Task_Either_TaskEither/taskeither.ts"
  },
  "dependencies": {
    "@contactlab/appy": "3.1.3",
    "axios": "^0.27.2",
    "fp-ts": "2.9.1",
    "fp-ts-contrib": "0.1.21",
    "fp-ts-laws": "0.3.0",
    "fp-ts-std": "0.7.0",
    "io-ts": "2.2.13",
    "io-ts-types": "0.5.12",
    "monocle-ts": "2.3.3",
    "newtype-ts": "0.3.4",
    "node-dev": "6.2.0",
    "ts-node": "9.1.1",
    "ts-node-dev": "1.1.0"
  },
  "devDependencies": {
    "@types/node": "^18.0.6",
    "typescript": "^4.1.0"
  },
  "keywords": [
    "fp-ts",
    "functional",
    "programming",
    "typescript"
  ],
  "author": "COANelmejor <coanelmejor@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/COANelmejor/Practical-Guide-to-Fp-ts/issues"
  },
  "homepage": "https://github.com/COANelmejor/Practical-Guide-to-Fp-ts#readme"
}
